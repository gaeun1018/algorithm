#include<iostream>
#include<string>

using namespace std;

int main() {
	int a = 0, num;
	cin >> num;
	for (int i = 1; i <= num; i++) {
		string nums = to_string(i);
        if (nums.size() < 3)
		    a++;
	    else if (nums[2] - nums[1] == nums[1] - nums[0])
		    a++;
	}
	cout << a;
}

